@model task5.Models.Department

<h2>@ViewBag.PageName</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Location"></label>
        <input asp-for="Location" class="form-control" />
        <span asp-validation-for="Location" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="ManagerId"></label>
        <input asp-for="ManagerId" class="form-control" />
        <span asp-validation-for="ManagerId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="EstablishedDate"></label>
        <input asp-for="EstablishedDate" class="form-control" />
        <span asp-validation-for="EstablishedDate" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary mt-5">Add</button>
</form>
